		<maven.compiler.source>1.8</maven.compiler.source>
		<sonar.projectKey>michaelbmcgroary_Lemira_Revature_Project_2</sonar.projectKey>
  		<sonar.organization>lemira</sonar.organization>
  		<sonar.host.url>https://sonarcloud.io</sonar.host.url>

		<!-- This is something we added ourselves for Jacoco to provide Code Coverage reports -->
		<!-- Jacoco is a Maven plugin that will be triggered when we do a Sonarcloud scan -->
		<sonar.coverage.jacoco.xmlReportPaths>target/site/jacoco/jacoco.xml</sonar.coverage.jacoco.xmlReportPaths>
	</properties>


